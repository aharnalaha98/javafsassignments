package Bean;
import java.util.Comparator;
public class Employee {
	private int id = -1;
    private String Name = null;
    
    private double Salary = -1;
 
    public Employee(int id, String Name, double Salary) {
        this.id = id;
        this.Name = Name;
        
        this.Salary = Salary;
    }
 
    // Setters and Getters
 
    @Override
    public String toString() {
        return "Employee : " + id + " - " + Name + " - " + Salary + "n";
    }
}



	